generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int     @id @default(autoincrement())
  type         String?
  username     String
  emal         String?
  accessToken  String? @map("access_token")
  refreshToken String? @map("refresh_token")
}

